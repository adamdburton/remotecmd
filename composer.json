{
    "name": "adamdburton/remotecmd",
    "description": "Runs commands and tasks over SSH.",
    "require": {
        "phpseclib/phpseclib": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "AdamDBurton\\RemoteCmd\\": "src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "friendsofphp/php-cs-fixer": "^2.14"
    },
    "scripts": {
        "format": [
            "vendor/bin/php-cs-fixer fix"
        ]
    }
}
